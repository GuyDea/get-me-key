{
  "name": "GetMeKey",
  "description": "Password generating utility",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["clipboardWrite", "clipboardRead"],
  "action": {
    "default_title": "GetMeKey",
    "default_icon": "static/icons/gmk_icon_128.png",
    "default_popup": "index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}