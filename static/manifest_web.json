{
  "name": "GetMeKey",
  "description": "The only password manager you'll ever need",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [],
  "action": {
    "default_title": "GetMeKey",
    "default_icon": "icons/gmk_icon_128.png"
  },
  "icons": [
    {
      "src": "icons/gmk_icon_512.png",
      "type": "image/png",
      "sizes": "512x512"
    },
    {
      "src": "icons/gmk_icon_512_maskable.png",
      "type": "image/png",
      "sizes": "512x512",
      "purpose": "maskable"
    }
  ],
  "start_url": "index.html",
  "scope": "/",
  "display": "standalone",
  "theme_color": "#E2D9FC",
  "background_color": "#E2D9FC"
}